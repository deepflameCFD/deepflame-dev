(
    Left
    {
        // type patch;
        type symmetryPlane;
        faces
        (
            (0 4 7 3)
        );
    }
    Right
    {
        // type patch;
        type symmetryPlane;
        faces
        (
            (1 2 6 5)
        );
    }
    empty
    {
        type empty;
        faces
        (
            (0 1 5 4)
            (5 6 7 4)
            (3 7 6 2)
            (0 3 2 1)
        );
    }
);

